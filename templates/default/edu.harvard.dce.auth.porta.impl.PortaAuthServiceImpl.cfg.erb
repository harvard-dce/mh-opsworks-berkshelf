
# Indicates if this service is enabled
enabled=<%= @enabled %>

# The url where the porta-auto service is (for authorization checks) e.g. https://porta-auto.dcex.harvard.edu
api.base.url=<%= @porta_auto_url %>

# API key to be used when calling the porta-auto api for authorization
api.key=<%= @porta_auto_api_key %>

# The porta-auto api path. Default: /api/v1/porta_api
#api.path=/api/v1/porta_auto

# Url to redirect when the user needs to log in. This is not a porta-auto url, but a web page
# that displays a message and has login/logout buttons.
# Something like: https://ENGAGE_HOST/engage/ui/index.html#/login
redirect.url=<%= @redirect_url %>

# Porta-auto session cookie name. In production, porta_auto_sessionid.
cookie.name=<%= @cookie_name %>

# Porta-auto session cookie domain. Default: .harvard.edu 
#cookie.domain=.harvard.edu

# Porta-auto session cookie path. Default: /
#cookie.path=/

# Login path. Path to porta-auto login url. Default: /cas/login
#login.path=/cas/login

# Logout path. Path to porta-auto logout url. Default: /logout
#logout.path=/logout

# HarvardKey logout path. Path to porta-auto cas logout url, which also logs out of HarvardKey.
# Default: /cas/logout
#logout.hkey.path=/cas/logout

