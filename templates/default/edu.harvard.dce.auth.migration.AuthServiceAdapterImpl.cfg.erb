
# Auth system to be used: "porta" or "acid"; if not entered, default is "acid" (legacy auth)
default.auth=<%= @default_auth_system %>

# Comma-separated list of courses that will NOT use the default auth service specified above.
# If default.auth=acid, courses in other.auth.courses will use "porta" auth and vice-versa.
# This is a comma-separated list.
other.auth.courses=<%= @other_courses %>

